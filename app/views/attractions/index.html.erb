<h1>Attractions Index Page</h1>
 
 <% if current_user.admin %>
   <%= link_to "New Attraction", new_attraction_path %>
   <% @attractions.each do |attraction| %>
   <h4><%= link_to "Show #{attraction.name}", attraction_path(attraction)%></h4>
   <% end %>
 <% else %>
   <% @attractions.each do |attraction| %>
   <h4><%= link_to "Go on #{attraction.name}", attraction_path(attraction)%></h4>
   <% end %>
 <% end %>

















